# Next.js Environment
NODE_ENV=development

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:4000
NEXT_PUBLIC_SERVER_URL=http://localhost:3000

# Agent Configuration
NEXT_PUBLIC_AGENT_ID=your-agent-id-here
NEXT_PUBLIC_AGENT_NAME=Bio Launchpad Agent
NEXT_PUBLIC_AGENT_DESCRIPTION=Bio Launchpad Agent for longevity research
NEXT_PUBLIC_AGENT_SHORT_DESCRIPTION=Your longevity co-pilot with expert AI guidance
NEXT_PUBLIC_AGENT_LOGO=/assets/bot.png
NEXT_PUBLIC_AGENT_BANNER_LOGO=/assets/agent-banner.png

# API Configuration
NEXT_PUBLIC_API_KEY=your-api-key-here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Privy Authentication
NEXT_PUBLIC_PRIVY_APP_ID=your-privy-app-id
PRIVY_APP_SECRET=your-privy-app-secret

# ElevenLabs (Text-to-Speech)
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_VOICE_ID=your-elevenlabs-voice-id

# Example Prompts (comma-separated)
NEXT_PUBLIC_EXAMPLE_PROMPTS=Tell me about longevity research,What are the latest advances in anti-aging?,How can I optimize my healthspan?

# Debug Mode
NEXT_PUBLIC_DEBUG=false
