# Next.js Environment
NODE_ENV=development

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:4000
NEXT_PUBLIC_SERVER_URL=http://localhost:3000

# === AGENT CONFIGURATION ===
# Core Agent Identity
NEXT_PUBLIC_AGENT_ID=your-agent-id-here
NEXT_PUBLIC_AGENT_NAME=Bio Agent
NEXT_PUBLIC_AGENT_DISPLAY_NAME=Bio Agent - AI Assistant
NEXT_PUBLIC_AGENT_DESCRIPTION=An intelligent AI assistant for research and analysis
NEXT_PUBLIC_AGENT_SHORT_DESCRIPTION=Your AI research companion
NEXT_PUBLIC_AGENT_DOMAIN=research

# Agent Branding & Theme
NEXT_PUBLIC_AGENT_THEME=default-theme
NEXT_PUBLIC_PRIMARY_COLOR=#3B82F6
NEXT_PUBLIC_SECONDARY_COLOR=#10B981
NEXT_PUBLIC_ACCENT_COLOR=#F59E0B

# Agent Assets
NEXT_PUBLIC_AGENT_LOGO=/assets/bot.png
NEXT_PUBLIC_AGENT_BANNER_LOGO=/assets/agent-banner.png
NEXT_PUBLIC_AGENT_LOGIN_IMAGE=/assets/login-image.png
NEXT_PUBLIC_AGENT_FAVICON=/favicon.png
NEXT_PUBLIC_AGENT_OG_IMAGE=/og.png

# Agent Content
NEXT_PUBLIC_WELCOME_MESSAGE=Hello! I'm your AI assistant, here to help you with research and analysis.
NEXT_PUBLIC_AGENT_TAGLINE=Your intelligent AI companion
NEXT_PUBLIC_EXAMPLE_PROMPTS=Tell me about recent research,Analyze this data,Help me understand this concept
NEXT_PUBLIC_AGENT_KEYWORDS=AI,research,analysis,assistant
NEXT_PUBLIC_ABOUT_CONTENT=Learn more about this AI agent and its capabilities.

# Agent Social Links
NEXT_PUBLIC_AGENT_X_USERNAME=
NEXT_PUBLIC_AGENT_DISCORD_SERVER=
NEXT_PUBLIC_AGENT_WEBSITE_URL=
NEXT_PUBLIC_AGENT_GITHUB_URL=

# Agent Features (true/false)
NEXT_PUBLIC_ENABLE_DEEP_RESEARCH=true
NEXT_PUBLIC_ENABLE_FILE_UPLOAD=true
NEXT_PUBLIC_ENABLE_TEXT_TO_SPEECH=true
NEXT_PUBLIC_ENABLE_VOICE_INPUT=true
NEXT_PUBLIC_ENABLE_VOTING=true

# === API CONFIGURATION ===
NEXT_PUBLIC_API_KEY=your-api-key-here
NEXT_PUBLIC_WORLD_ID=00000000-0000-0000-0000-000000000000

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Privy Authentication
NEXT_PUBLIC_PRIVY_APP_ID=your-privy-app-id

# ElevenLabs (Text-to-Speech)
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_VOICE_ID=your-elevenlabs-voice-id
ELEVENLABS_VOICE_STABILITY=0.5
ELEVENLABS_VOICE_SIMILARITY_BOOST=0.75
ELEVENLABS_VOICE_STYLE=0
ELEVENLABS_VOICE_USE_SPEAKER_BOOST=true
ELEVENLABS_MODEL_ID=eleven_monolingual_v1
ELEVENLABS_OUTPUT_FORMAT=pcm_16000
ELEVENLABS_OPTIMIZE_STREAMING_LATENCY=4

# Email Service (Resend)
RESEND_API_KEY=your-resend-api-key

# Analytics (PostHog)
NEXT_PUBLIC_POSTHOG_KEY=your-posthog-key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Debug Mode
NEXT_PUBLIC_DEBUG=false
NEXT_TELEMETRY_DISABLED=true
